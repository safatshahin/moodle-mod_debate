<div class="mod-debate-find-response-container mod-debate-find-response">
    <div class="mod-debate-find-response-element">
        <div class="font-weight-bold">
            {{#str}} possible_match, mod_debate{{/str}}
        </div>
    </div>
    {{^found_response}}
        <div class="p-3">
            <div class="mod-debate-response-text mod-debate-find-response-text">
                {{#str}} no_possible_match, mod_debate{{/str}}
            </div>
        </div>
    {{/found_response}}
    {{#found_response}}
        <div class="p-3">
            <div class="mod-debate-response-text mod-debate-find-response-text">
                {{response}}
            </div>
        </div>
    {{/found_response}}
</div>
